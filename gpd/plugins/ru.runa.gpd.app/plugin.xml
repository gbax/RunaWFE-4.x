<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
    <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="ru.runa.gpd.application"
            name="runa-gpd">
         <property
               name="windowImages"
               value="icons/logo/16.gif,icons/logo/32.gif,icons/logo/48.gif">
         </property>
         <property
               name="aboutText"
               value="Runa Developer Studio">
         </property>
         <property
               name="aboutImage"
               value="icons/logo/about.gif">
         </property>
         <property
               name="appName"
               value="runa-gpd">
         </property>
      </product>
   </extension>
    <extension
          point="org.eclipse.ui.splashHandlers">
       <splashHandler
             class="ru.runa.gpd.app.VersionSplashHandler"
             id="ru.runa.gpd.app.versionSsplashHandler">
       </splashHandler>
       <splashHandlerProductBinding
             productId="ru.runa.gpd.app.product"
             splashId="ru.runa.gpd.app.versionSsplashHandler">
       </splashHandlerProductBinding>
    </extension>


</plugin>
