<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

    <extension
         point="ru.runa.gpd.handlers">
      <handler
            cellEditorProvider="ru.runa.gpd.office.excel.ReadExcelHandlerCellEditorProvider"
            className="ru.runa.wf.office.excel.handlers.ReadHandler"
            label="%ActionHandler.ReadHandler"
            enabled="true"
            type="actionHandler">
      </handler>
      <handler
            cellEditorProvider="ru.runa.gpd.office.excel.SaveExcelHandlerCellEditorProvider"
            className="ru.runa.wf.office.excel.handlers.SaveHandler"
            label="%ActionHandler.SaveHandler"
            enabled="true"
            type="actionHandler">
      </handler>
      <handler
            cellEditorProvider="ru.runa.gpd.office.word.DocxHandlerCellEditorProvider"
            className="ru.runa.wf.office.doc.DocxHandler"
            label="%ActionHandler.DocxHandler"
            enabled="true"
            type="actionHandler">
      </handler>
   </extension>

</plugin>
