<?xml version="1.0" encoding="UTF-8"?>

<validators>
  <validator type="expression">
    <message><![CDATA[Till should be more or equals since data]]></message>
    <param name="expression"><![CDATA[till.getTime() >= since.getTime()]]></param>
  </validator>
  <field name="businessTripType">
    <field-validator type="required">
      <message><![CDATA[Field is required]]></message>
    </field-validator>
  </field>
  <field name="reason">
    <field-validator type="stringlength">
      <message><![CDATA[Length cannot be more than 100 symbols]]></message>
      <param name="maxLength"><![CDATA[100]]></param>
    </field-validator>
    <field-validator type="required">
      <message><![CDATA[Field is required]]></message>
    </field-validator>
  </field>
  <field name="since">
    <field-validator type="required">
      <message><![CDATA[Field is required]]></message>
    </field-validator>
  </field>
  <field name="comment">
    <field-validator type="stringlength">
      <message><![CDATA[Length cannot be more than 250 symbols]]></message>
      <param name="maxLength"><![CDATA[250]]></param>
    </field-validator>
  </field>
  <field name="till">
    <field-validator type="required">
      <message><![CDATA[Field is required]]></message>
    </field-validator>
  </field>
  <field name="staffrole">
    <field-validator type="required">
      <message><![CDATA[Field is required]]></message>
    </field-validator>
  </field>
</validators>
