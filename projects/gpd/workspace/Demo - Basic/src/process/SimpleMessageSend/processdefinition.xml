<?xml version="1.0" encoding="UTF-8"?>

<process-definition name="SimpleMessageSend">
  <swimlane id="manager" name="manager">
    <assignment class="ru.runa.wfe.handler.assign.DefaultAssignmentHandler"><![CDATA[]]></assignment>
  </swimlane>
  <start-state id="Start" name="Start">
    <task name="Start" swimlane="manager"/>
    <transition id="tr1" name="tr1" to="Input data"/>
  </start-state>
  <task-node id="Input data" name="Input data" async="false">
    <task name="Input data" swimlane="manager"/>
    <transition id="tr1" name="tr1" to="Send data"/>
  </task-node>
  <send-message id="Send data" name="Send data">
    <transition id="tr1" name="tr1" to="End"/>
    <variable name="processDefinitionName" mapped-name="SimpleMessageReceive" access="selector"/>
    <variable name="msg_send" mapped-name="msg_send" access="read"/>
  </send-message>
  <end-state id="End" name="End"/>
</process-definition>
