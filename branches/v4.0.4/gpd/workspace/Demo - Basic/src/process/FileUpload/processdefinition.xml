<?xml version="1.0" encoding="UTF-8"?>

<process-definition name="FileUpload">
  <description><![CDATA[This is sample workflow process demonstrates work with file]]></description>
  <swimlane id="user" name="user">
    <assignment class="ru.runa.wfe.extension.assign.DefaultAssignmentHandler"><![CDATA[]]></assignment>
  </swimlane>
  <start-state id="upload" name="upload">
    <description><![CDATA[User uploads file]]></description>
    <task name="upload" swimlane="user"/>
    <transition id="tr1" name="tr1" to="download"/>
  </start-state>
  <task-node id="download" name="download" async="false">
    <description><![CDATA[User downloads file]]></description>
    <task name="download" swimlane="user"/>
    <transition id="tr1" name="tr1" to="end"/>
  </task-node>
  <end-state id="end" name="end"/>
</process-definition>
