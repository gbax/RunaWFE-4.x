<?xml version="1.0" encoding="UTF-8"?>

<process-definition name="MainProcess">
  <description><![CDATA[MainProcess for subprocess demonstration (no start form provided)]]></description>
  <swimlane id="a_role" name="a_role">
    <assignment class="ru.runa.wfe.extension.assign.DefaultAssignmentHandler"><![CDATA[]]></assignment>
  </swimlane>
  <start-state id="start" name="start">
    <task name="start" swimlane="a_role"/>
    <transition id="tr1" name="tr1" to="before subprocess"/>
  </start-state>
  <task-node id="before subprocess" name="before subprocess" async="false">
    <description><![CDATA[before subprocess]]></description>
    <task name="before subprocess" swimlane="a_role"/>
    <transition id="tr1" name="tr1" to="process-state"/>
  </task-node>
  <task-node id="after subprocess" name="after subprocess" async="false">
    <description><![CDATA[after subprocess]]></description>
    <task name="after subprocess" swimlane="a_role"/>
    <transition id="tr1" name="tr1" to="end"/>
  </task-node>
  <process-state id="process-state" name="process-state">
    <transition id="tr1" name="tr1" to="after subprocess"/>
    <sub-process name="SubProcess"/>
    <variable name="a_role" mapped-name="role1" access="read"/>
    <variable name="number" mapped-name="n" access="read,write"/>
    <variable name="date" mapped-name="d" access="read,write"/>
  </process-state>
  <end-state id="end" name="end"/>
</process-definition>
