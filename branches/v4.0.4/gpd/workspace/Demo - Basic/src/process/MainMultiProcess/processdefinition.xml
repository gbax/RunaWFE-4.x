<?xml version="1.0" encoding="UTF-8"?>

<process-definition name="MainMultiProcess">
  <description><![CDATA[MainProcess for subprocess demonstration (no start form provided)]]></description>
  <swimlane id="a_role" name="a_role">
    <assignment class="ru.runa.wfe.extension.assign.DefaultAssignmentHandler"><![CDATA[]]></assignment>
  </swimlane>
  <start-state id="start" name="start">
    <task name="start" swimlane="a_role"/>
    <transition id="tr1" name="tr1" to="before subprocess"/>
  </start-state>
  <task-node id="before subprocess" name="before multi subprocess" async="false">
    <description><![CDATA[before subprocess]]></description>
    <task name="before multi subprocess" swimlane="a_role"/>
    <transition id="tr1" name="tr1" to="ID3"/>
  </task-node>
  <task-node id="after subprocess" name="after multi subprocess" async="false">
    <description><![CDATA[after subprocess]]></description>
    <task name="after multi subprocess" swimlane="a_role"/>
    <transition id="tr1" name="tr1" to="end"/>
  </task-node>
  <multiinstance-state id="ID3" name="Multi subprocess">
    <transition id="tr1" name="tr1" to="after subprocess"/>
    <sub-process name="SubProcess"/>
    <variable name="dates" mapped-name="d" access="read,write"/>
    <variable name="numbers" mapped-name="n" access="read,write"/>
    <variable name="tabVariableProcessVariable" mapped-name="actors" access="multiinstance-vars"/>
    <variable name="tabVariableSubProcessVariable" mapped-name="role1" access="multiinstance-vars"/>
    <variable name="typeMultiInstance" mapped-name="variable" access="multiinstance-vars"/>
  </multiinstance-state>
  <end-state id="end" name="end"/>
</process-definition>
