<?xml version="1.0" encoding="UTF-8"?>

<process-definition  xmlns="urn:jbpm.org:jpdl-3.2"  name="file upload sample">


 <description>This is sample workflow process demonstrates work with file.</description>
 	<swimlane name="requester"></swimlane>


	<start-state name="upload">
		<task swimlane="requester">		
		</task>
		<transition to="download"></transition>
	</start-state>


	<task-node name="download">
		<task swimlane="requester"></task>
		<transition to="endstate"></transition>
	</task-node>


	<end-state name="endstate"></end-state>


</process-definition>