<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-access="field" >

  <subclass name="ru.runa.bpm.graph.node.Decision" 
            discriminator-value="D" 
            extends="ru.runa.bpm.graph.def.Node">
            
    <property name="decisionExpression" column="DECISIONEXPRESSION_"/>

    <many-to-one name="decisionDelegation"
                 class="ru.runa.bpm.instantiation.Delegation"
                 column="DECISIONDELEGATION"
                 foreign-key="FK_DECISION_DELEG"
                 cascade="all" lazy="false" />

  </subclass>

</hibernate-mapping>
