<portlet-app id="jsf-demo" 
    xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd" version="1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd 
    http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd">        
    
    <portlet id="RunaWFE">
        <description>RunaWFE</description>
        <portlet-url-class>org.apache.portals.bridges.struts.StrutsPortletURL</portlet-url-class>
        <portlet-name>RunaWFE</portlet-name>
        <display-name>RunaWFE</display-name>
        <portlet-class>ru.runa.common.web.portlet.RunaWFEPortlet</portlet-class>
        <init-param>
      		<name>ViewPage</name>
		<value>/start.do</value>
        </init-param>

        <init-param>
            <name>StrutsPortletConfigLocation</name> 
            <value>WEB-INF/struts-portlet-config.xml</value> 
        </init-param>
        <init-param>
            <name>ServletContextProvider</name> 
            <value>org.jboss.portal.portlet.bridge.JBossServletContextProvider</value> 
        </init-param>
	<init-param>
		<name>PortletScopeStrutsSession</name>
		<value>true</value>
	</init-param>
        <init-param>
      		<name>runawfe.authenticator</name>
		<value>ru.runa.common.web.portlet.impl.AuthenticateIfPossible</value>
        </init-param>
        <init-param>
      		<name>runawfe.exceptionHandler</name>
		<value>ru.runa.common.web.portlet.impl.DefaultExceptionHandler</value>
        </init-param>

        <init-param>
      		<name>runawfe.thinInterface</name>
		<value></value>
        </init-param>

        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>VIEW</portlet-mode>
        </supports>
        <portlet-info>
            <title>RunaWFE</title>
            <short-title>RunaWFE</short-title>
            <keywords>RunaWFE, workflow</keywords>            
        </portlet-info>
        <resource-bundle>struts</resource-bundle>            
    </portlet>

