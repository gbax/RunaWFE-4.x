<?xml version="1.0" encoding="UTF-8"?>
<process-definition name="FileUpload" xmlns="urn:jbpm.org:jpdl-3.2">
  <description><![CDATA[This is sample workflow process demonstrates work with file]]></description>
  <swimlane name="user">
    <assignment
      class="ru.runa.wf.jbpm.delegation.assignment.AssignmentHandler" config-type="configuration-property"><![CDATA[]]></assignment>
  </swimlane>
  <start-state name="upload">
    <description><![CDATA[User uploads file]]></description>
    <task name="upload" swimlane="user"/>
    <transition name="tr1" to="download"/>
  </start-state>
  <task-node name="download">
    <description><![CDATA[User downloads file]]></description>
    <task name="download" swimlane="user"/>
    <transition name="tr1" to="end"/>
  </task-node>
  <end-state name="end"/>
</process-definition>
