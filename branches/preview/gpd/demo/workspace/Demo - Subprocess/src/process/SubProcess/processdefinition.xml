<?xml version="1.0" encoding="UTF-8"?>
<process-definition name="SubProcess" xmlns="urn:jbpm.org:jpdl-3.2">
  <description><![CDATA[SubProcess for subprocess demonstration]]></description>
  <swimlane name="startedrole">
    <assignment
      class="ru.runa.wf.jbpm.delegation.assignment.AssignmentHandler" config-type="configuration-property"><![CDATA[]]></assignment>
  </swimlane>
  <swimlane name="role1">
    <assignment
      class="ru.runa.wf.jbpm.delegation.assignment.AssignmentHandler" config-type="configuration-property"><![CDATA[]]></assignment>
  </swimlane>
  <start-state name="start">
    <task name="start" swimlane="startedrole"/>
    <transition name="tr1" to="fork1"/>
  </start-state>
  <task-node name="state1">
    <description><![CDATA[state1 of subprocess]]></description>
    <task name="state1" swimlane="role1"/>
    <transition name="tr1" to="join1"/>
  </task-node>
  <task-node name="state2">
    <description><![CDATA[state2 of subprocess]]></description>
    <task name="state2" swimlane="role1"/>
    <transition name="tr1" to="join1"/>
  </task-node>
  <fork name="fork1">
    <transition name="tr1" to="state2"/>
    <transition name="tr2" to="state1"/>
  </fork>
  <join name="join1">
    <transition name="tr1" to="end"/>
  </join>
  <end-state name="end"/>
</process-definition>
