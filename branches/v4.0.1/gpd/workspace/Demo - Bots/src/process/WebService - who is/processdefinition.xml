<?xml version="1.0" encoding="UTF-8"?>

<process-definition name="WebService - who is">
  <description><![CDATA[Enter domain name and WebService bot resolve this name using whois service.]]></description>
  <swimlane id="ID0" name="requester">
    <assignment class="ru.runa.wfe.extension.assign.DefaultAssignmentHandler"><![CDATA[]]></assignment>
  </swimlane>
  <swimlane id="ID1" name="WebServiceBot">
    <assignment class="ru.runa.wfe.extension.assign.DefaultAssignmentHandler"><![CDATA[ru.runa.wfe.extension.orgfunction.ExecutorByNameFunction(WebServiceBot)]]></assignment>
  </swimlane>
  <start-state id="Start" name="Start">
    <task name="Start" swimlane="requester"/>
    <transition id="ID2" name="tr1" to="Load whois information"/>
  </start-state>
  <task-node id="Load whois information" name="Load whois information" async="false">
    <task name="Load whois information" swimlane="WebServiceBot">
      <event type="task-create">
        <action class="ru.runa.wfe.service.handler.BotInvokerActionHandler"><![CDATA[]]></action>
      </event>
    </task>
    <transition id="ID3" name="tr1" to="Read DNS address information"/>
  </task-node>
  <task-node id="Read DNS address information" name="Read DNS address information" async="false">
    <task name="Read DNS address information" swimlane="requester"/>
    <transition id="ID5" name="tr1" to="End"/>
  </task-node>
  <end-state id="End" name="End"/>
</process-definition>
