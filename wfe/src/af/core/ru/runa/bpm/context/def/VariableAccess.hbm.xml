<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-access="field" >

  <class name="ru.runa.bpm.context.def.VariableAccess" 
         table="JBPM_VARIABLEACCESS">
    <cache usage="read-write" />
    <id name="id" column="ID_">
	   	<generator class="native">
	        <param name="sequence">SEQ_JBPM_VARIABLEACCESS</param>
	    </generator>
    </id>
    
    <property name="variableName" column="VARIABLENAME_" />
    <property name="access" column="ACCESS_" type="ru.runa.bpm.db.hibernate.AccessType" />
    <property name="mappedName" column="MAPPEDNAME_" />
  </class>

</hibernate-mapping>
