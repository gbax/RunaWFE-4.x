<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-access="field" >

  <subclass name="ru.runa.bpm.context.log.variableinstance.StringUpdateLog" 
            extends="ru.runa.bpm.context.log.VariableUpdateLog"
            discriminator-value="U">
    <property name="oldValue" column="OLDSTRINGVALUE_" type="text" length="1024" />
    <property name="newValue" column="NEWSTRINGVALUE_" type="text" length="1024" />
  </subclass>

</hibernate-mapping>
