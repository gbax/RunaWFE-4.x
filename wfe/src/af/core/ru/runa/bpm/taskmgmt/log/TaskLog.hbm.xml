<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-access="field" >

  <subclass name="ru.runa.bpm.taskmgmt.log.TaskLog" 
            extends="ru.runa.bpm.logging.log.ProcessLog"
            discriminator-value="0">

    <many-to-one name="taskInstance" 
                 column="TASKINSTANCE_" 
                 class="ru.runa.bpm.taskmgmt.exe.TaskInstance" 
                 foreign-key="FK_LOG_TASKINST" />
  </subclass>

</hibernate-mapping>
