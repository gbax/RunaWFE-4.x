<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE process-definition PUBLIC
"-//jBpm/jBpm Mapping DTD 2.0//EN"
"http://jbpm.org/dtd/processdefinition-2.0.dtd">
<process-definition name="HelloSQLTaskHandler">

   <swimlane name="bot"><delegation class="ru.runa.wf.jbpm.delegation.assignment.AssignmentHandler">ru.runa.af.organizationfunction.ExecutorByNameFunction(bot)</delegation></swimlane>
   <swimlane name="requester"><delegation class="ru.runa.wf.jbpm.delegation.assignment.AssignmentHandler"></delegation></swimlane>
   <start-state name="start" swimlane="requester">
      <transition name="tr1" to="store data"></transition>
   </start-state>

   <state name="store data">
      <assignment swimlane="bot"></assignment><transition name="tr1" to="end"></transition>
   </state>


   <end-state name="end"></end-state>
</process-definition>