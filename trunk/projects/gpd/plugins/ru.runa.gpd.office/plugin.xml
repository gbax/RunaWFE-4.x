<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

    <extension
         point="ru.runa.gpd.handlers">
      <handler
            cellEditorProvider="ru.runa.gpd.office.excel.ReadHandlerCellEditorProvider"
            className="ru.runa.wf.office.excel.handlers.ReadHandler"
            displayName="%ActionHandler.ReadHandler"
            enabled="true"
            type="actionHandler">
      </handler>
      <handler
            cellEditorProvider="ru.runa.gpd.office.excel.SaveHandlerCellEditorProvider"
            className="ru.runa.wf.office.excel.handlers.SaveHandler"
            displayName="%ActionHandler.SaveHandler"
            enabled="true"
            type="actionHandler">
      </handler>
      <handler
            cellEditorProvider="ru.runa.gpd.office.word.DocxHandlerCellEditorProvider"
            className="ru.runa.wf.office.doc.DocxHandler"
            displayName="%ActionHandler.DocxHandler"
            enabled="true"
            type="actionHandler">
      </handler>
   </extension>

</plugin>
